							}
						},
						yAxis : {
							title : {
								text : '用户数'
							},
							min : 0
						},
						tooltip : {
							formatter : function() {
								return '<b>' + this.series.name + '</b>'+Highcharts.dateFormat('%H:%M', this.x) +"  "+ this.y + '个';
							}
